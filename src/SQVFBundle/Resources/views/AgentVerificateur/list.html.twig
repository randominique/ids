{% extends "base.html.twig" %}

{% block title %}Agent_Verificateur{% endblock %}

{% block page_title %}
  <h1 class="mytitle">Agent_Verificateur</h1>
{% endblock %}
{% block page_subtitle %}{% endblock %}

{% block page_content %}
<div class="box box-primary">
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>id</th>	
        <th>idDossier</th>	
        <th>idAgentVerificateur</th>	
      </tr>
    </thead>
    <tbody>
      {% for agent in agents %}																								
        <tr>
          <td>{{ agent.id|number_format(0,',',' ') }}</td>
          <td>{{ agent.idDossier }}</td>
          <td>{{ agent.idAgentVerificateur }}</td>
        </tr>
      {% endfor %}
    </tbody>
  </table>
</div>
{% endblock %}
