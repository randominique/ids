{% extends "base.html.twig" %}
{% block title %}Taxe type: {{ taxe.name }}{% endblock %}

{% block page_title %}Taxe type: {{ taxe.name }}{% endblock %}
{% block page_subtitle %}{% endblock %}

{% block page_content %}

    <div class="box box-primary">
        <div class="box-header with-border">
            <p class="pull-right"><a href="{{ path('new_member_assujettissement_setting', {'taxe':taxe.id}) }}" class="btn btn-primary">Ajouter</a></p>
            <h3 class="box-title">Tax</h3>
        </div>
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>Membres</th>
                </tr>
            </thead>
            <tbody>
            {% for membre in membres %}
                <tr>
                    <td>{{ membre.taxeName.taxTypeDesc }}</td>
                </tr>
            {% endfor %}
            <tbody>
        </table>
    </div>

{% endblock %}