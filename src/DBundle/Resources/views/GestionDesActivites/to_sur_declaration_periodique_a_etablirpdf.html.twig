
{% block title %}Taxation d'office - A établir{% endblock %}

{% block page_title %}Taxation d'office{% endblock %}
{% block page_subtitle %}A établir{% endblock %}

{% block page_content %}

    <div class="box box-primary">
      
        <table class="table table-striped table-hover">
            <thead>
                <tr style="font-weight:bold">
                    <th>NIF</th>
                    <th>Raison sociale</th>
                    <th>Type d'impôt</th>
                    <th>Date d'entrée</th>
                    <th>Montant</th>
                  
                </tr>
            </thead>
            <tbody>
            {% for to in pagination %}
                <tr>
                    <td>{{ to.nif }}</td>
                    <td>{{ to.rs }}</td>
                    <td>{{to.taxTypeDescF}}</td>
                    <td>{{ to.entryDate | date('d/m/Y')}}</td> 
                    <td>{{ to.taxe }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
            
        <div class="navigation">
            {{ knp_pagination_render(pagination) }}
        </div>
    </div>

{% endblock %}