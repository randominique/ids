{% extends "base.html.twig" %}

{% block title %}Utilisteurs SQVF{% endblock %}

{% block page_title %}
  <h1 class="mytitle">Utilisteurs SQVF</h1>
{% endblock %}
{% block page_subtitle %}{% endblock %}

{% block page_content %}
<div class="box box-primary">
  <div class="info-box-content">
      <span class="info-box-text">Utilisteurs SQVF</span>
      <span class="info-box-number">{{ nbreContrib }}</span>
  </div>
  {# <div class="navigation">
    {{ knp_pagination_render(users) }}
  </div> #}
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th class="form-entete">idTypeUser</th>	
        <th class="form-entete">idCentreFiscal</th>	
        <th class="form-entete">idChef</th>	
        <th class="form-entete">firstName</th>
        <th class="form-entete">lastName</th>
        <th class="form-entete">fullName</th>
        <th class="form-entete">email</th>
        <th class="form-entete">password</th>
        <th class="form-entete">hash</th>
        <th class="form-entete">lastActivity</th>
        <th class="form-entete">archive</th>
        <th class="form-entete">createTime</th>
        <th class="form-entete">updateTime</th>
      </tr>
    </thead>
    <tbody>
      {% for user in users %}																								
        <tr>
          <td>{{ user.idTypeUser }}</td>
          <td>{{ user.idCentreFiscal }}</td>
          <td>{{ user.idChef }}</td>
          <td>{{ user.firstName }}</td>
          <td>{{ user.lastName }}</td>
          <td>{{ user.fullName }}</td>
          <td>{{ user.email }}</td>
          <td>{{ user.password }}</td>
          <td>{{ user.hash }}</td>
          <td>{{ user.lastActivity|date('Y-m-d H:i:s') }}</td>
          <td>{{ user.archive }}</td>
          <td>{{ user.createTime|date('Y-m-d H:i:s') }}</td>
          <td>{{ user.updateTime|date('Y-m-d H:i:s') }}</td>
        </tr>
      {% endfor %}
    </tbody>
  </table>
</div>
{% endblock %}